		"player.parallel_1": {
			"states": {
				"default": {
					"animations": ["state"],
					"transitions": [
						{"吃1": "q.is_eating"}
					]
				},
				"吃1": {
					"animations": [
						{"use_mainhand:pre_eat": "v.main_eat"},
						{"use_offhand:pre_eat": "1-v.main_eat"}
					],
					"on_entry": ["v.eat=ysm.food_level;"],
					"transitions": [
						{"吃2": "q.all_animations_finished"},
						{"吃后": "!q.is_eating"}
					]
				},
				"吃2": {
					"animations": [
						{"use_mainhand:pre_eat2": "v.main_eat"},
						{"use_offhand:pre_eat2": "1-v.main_eat"}
					],
					"transitions": [
						{"吃后": "!q.is_eating"}
					]
				},
				"吃后": {
					"animations": [
						{"use:eat_after": "(ysm.food_level>=20&&(ysm.food_level-v.eat)>0)?1:0"}
					],
					"transitions": [
						{"default": "q.all_animations_finished"}
					]
				}
			}
