		"player.parallel_0": {
			"states": {
				"default": {
					"animations": ["parallel0", "parallel2", "parallel3", "parallel1", "parallel4", "parallel5", "parallel6", "parallel7"]
				}
			}
		},
		"player.parallel_4": {
			"states": {
				"default": {
					"animations": ["state"],
					"transitions": [
						{"全进去了": "(v.z_d==1?1:0)&&v.z_cs"},
						{"只露个头": "(v.z_d==2?1:0)&&v.z_cs"},
						{"出来了": "(v.z_d==0?1:0)&&v.z_cs"}
					]
				},
				"全进去了": {
					"animations": ["（全进去了）狐狐钻地动画"],
					"on_exit": ["v.z_cs=0;"],
					"transitions": [
						{"default": "q.all_animations_finished"}
					],
					"blend_transition": 0.05
				},
				"只露个头": {
					"animations": ["（只钻了个头）狐狐钻地动画"],
					"on_exit": ["v.z_cs=0;"],
					"transitions": [
						{"default": "q.all_animations_finished"}
					],
					"blend_transition": 0.05
				},
				"出来了": {
					"animations": ["（出来了）狐狐钻地动画"],
					"on_exit": ["v.z_cs=0;"],
					"transitions": [
						{"default": "q.all_animations_finished"}
					],
					"blend_transition": 0.05
				}
			}
		},
		"player.parallel_5": {
			"states": {
				"default": {
					"animations": ["state"],
					"transitions": [
						{"末地烛攻击动画": "v.swing_hand&&v.roaming.end_rod"}
					]
				},
				"末地烛攻击动画": {
					"animations": [" huhu_end_rod"],
					"on_entry": ["v.swing_hand=0;"],
					"sound_effects": [
						{"effect": ""}
					],
					"transitions": [
						{"default": "q.all_animations_finished"}
					]
				}
			}
		},
		"player.parallel_6": {},
		"player.parallel_7": {},
		"player.parallel_2": {
			"states": {
				"default": {
