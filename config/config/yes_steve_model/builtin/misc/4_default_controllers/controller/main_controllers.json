					"transitions": [
						{"default": "q.all_animations_finished"},
						{"attack1": "v.swing_sword&&!v.jump&&!(q.vertical_speed>0)"},
						{"attack_jump": "v.swing_sword&&v.jump"}
					],
					"blend_transition": 0.05,
					"blend_via_shortest_path": true
				},
				"fishing": {
					"animations": ["swing:fishing_rod"],
					"transitions": [
						{"not_fishing": "!ysm.is_fishing"}
					]
				},
				"not_fishing": {
